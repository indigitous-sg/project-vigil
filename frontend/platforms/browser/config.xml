<?xml version='1.0' encoding='utf-8'?>
<widget id="org.indigitous.projectvigil" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Project Vigil</name>
    <description>        Project vigil, based on a basic Vue.js + Framework7 template for PhoneGap.    </description>
    <author email="developer@indigitous.sg" href="https://github.com/indigitous-sg">        Team Vigil    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="phonegap-version" value="cli-6.1.0" />
</widget>
